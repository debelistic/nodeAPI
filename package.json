  "bugs": {
    "url": "https://github.com/debelistic/nodeAPI/issues"
  },
  "homepage": "https://github.com/debelistic/nodeAPI#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-watch": "^7.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.18.1"
  }
